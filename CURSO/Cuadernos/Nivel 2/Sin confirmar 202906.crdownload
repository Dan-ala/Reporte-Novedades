    print (f"Nombre: {self.__nombre}, Nota1: {self.__nota1}, Nota2: {self.__nota2}, Promedio:{self.obtener_nota_promedio()}, estudio en:{self.institucion}")

  def __str__(self):
    return self.__nombre + " -> " + str(self.obtener_nota_promedio())

  @classmethod
  def cambiar_institucion(cls, nueva_inst):
    cls.institucion = nueva_inst

  @classmethod
  def retornar_numero_estudiantes(cls):
    return cls.num_est

  @staticmethod
  def ver_escala():
    tabla = [
              ["Nota", "Escala"],
              ["0 a 2.9", "Baja"],
              ["3 a 3.9", "Media"],
              ["4 a 4.5", "Alta"],
              ["4.6 a 5", "Sobresaliente"],
            ]
    print (tabulate(tabla, tablefmt="fancy_grid"))

################################################################
while True:
  try:
    e1 = Estudiante (input("Nombre:"), float(input("Nota1:")),float(input("Nota2:")))
    e1.mostrar_informacion()
    e2 = Estudiante (input("Nombre:"), float(input("Nota1:")),float(input("Nota2:")))
    e2.mostrar_informacion()
    Estudiante.cambiar_institucion("unal")
    e1.mostrar_informacion()
    e2.mostrar_informacion()
    print ("Estudiantes: ", Estudiante.retornar_numero_estudiantes())
    Estudiante.ver_escala()

    break
  except Exception as e:
